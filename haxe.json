{
    "meta":
    {
        "name": "moon-run",
        "license": "MIT",
        "tags": ["utility"],
        "description": "Command-line utility for building and running Haxe projects with nested settings",
        "contributors": ["profound7"],
        "releasenote": "First relase",
        "version": "0.1.0",
        "url": "https://github.com/profound7/moon-run",
        "classPath": "src/"
    },
    
    "build":
    {
        "-main": "moon.run.Run",
        "-cp": ["src"],
        "-lib": ["mcli"],
        "-v": false,
        "-neko": "run.n"
    },
    
    "run":
    {
        "main":
        [
            ["hx", "build"]
        ]
    }
}